{
  "name": "colour-contrast-checker",
  "version": "3.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.js",
    "format": "prettier \"*/**/*.{js,jsx,ts,tsx,json,md,css,scss}\" --write",
    "lint": "run-p lint:*",
    "lint:css": "stylelint \"./**/*.{css,scss}\" --fix",
    "lint:js": "eslint \"*/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "new:c": "cross-env HYGEN_TMPLS=generators hygen new component",
    "new:r": "cross-env HYGEN_TMPLS=generators hygen new route",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@netlify/remix-adapter": "^2.4.0",
    "@remix-run/express": "^2.10.2",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "isbot": "^5.1.12",
    "lodash.round": "^4.0.4",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@remix-run/dev": "^2.10.2",
    "@types/chroma-js": "^2.4.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/lodash.round": "^4.0.9",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "autoprefixer": "^10.4.19",
    "chroma-js": "^2.4.2",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.7.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "hygen": "^6.2.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.6.0",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.3",
    "remix-development-tools": "^4.2.2",
    "stylelint": "^16.7.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-use-logical": "^2.1.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": ">=20"
  },
  "volta": {
    "node": "20.12.0"
  },
  "overrides": {
    "@csstools/media-query-list-parser": "2.1.3",
    "eslint": "^9.7.0"
  }
}
