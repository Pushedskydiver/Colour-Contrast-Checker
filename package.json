{
  "name": "colour-contrast-checker",
  "version": "2.0.0",
  "description": "WCAG accessibility colour contrast checker",
  "license": "ISC",
  "author": {
    "name": "Alex Clapperton",
    "email": "hi@alexclapperton.co.uk",
    "url": "https://alexclapperton.co.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pushedskydiver/Colour-Contrast-Checker"
  },
  "dependencies": {
    "chroma-js": "2.4.2",
    "history": "^5.3.0",
    "lodash.round": "^4.0.4",
    "lodash.throttle": "^4.1.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.0",
    "react-scripts": "^5.0.1",
    "styled-components": "5.3.6",
    "typescript": "^5.0.4",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.0",
    "@types/lodash.round": "^4.0.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/webfontloader": "^1.6.35",
    "eslint-plugin-react": "7.32.2",
    "source-map-explorer": "2.5.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "indent": [
            1,
            2
          ]
        }
      }
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "npm run build && serve -s build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
